      <section class="content-main">
          <div class="content-header">
              <h2 class="content-title">Orders List</h2>
              
            </div>
            <!-- <P style="color: rgb(47, 215, 38); text-align: center;"><%# msg %></P> -->

        <div class="card mb-4">
          <header class="card-header">
            <P style="color: rgb(17, 114, 12); text-align: center;"><%= msg %></P>
            <div class="row gx-3">
              <div class="col-lg-4 col-md-6 me-auto">
                <input
                  type="text"
                  placeholder="Search..."
                  class="form-control"
                />
              </div>
              
              <!-- <div class="d-gridcol-lg-4 col-md-6 me-auto"> <a href="/admin/addproduct">
                <button  class="btn btn-primary"><i class="icon material-icons md-add_box"></i>Add Product</button></a>
            </div> -->
            </div>
          </header>
          <!-- card-header end// -->
          <div class="card-body">
            <div class="table-responsive">
                <table class="table table-hover" style="width: 100%; text-align: center;">
                    <thead>
                      <tr>
                        <th style="width: 10%; text-align: center;">SI NO</th>
                        <th style="width: 10%; text-align: center;">Name</th>
                        <th style="width: 20%; text-align: center;">Email</th>
                        <th style="width: 10%; text-align: center;">Total</th>
                        <th style="width: 15%; text-align: center;">Date</th>
                        <th style="width: 15%; text-align: center;">Status</th>
                        <th style="width: 15%; text-align: center;">Action</th>
                      </tr>
                    </thead>
                    <tbody id="tableDatas">
                      <% for(let i=0; i<order.length; i++) { %>
                        <tr>
                          <td style="vertical-align: middle; text-align: center;"><%= i+1 %></td>
                          <td style="vertical-align: middle; text-align: center;"><%= order[i].billingDetails.name %></td>
                          <td style="vertical-align: middle; text-align: center;"><%= order[i].billingDetails.email %></td>
                          <td style="vertical-align: middle; text-align: center;"><%= order[i].totalPrice %></td>
                          <td style="vertical-align: middle; text-align: center;"><%= order[i].orderDate.toLocaleDateString() %></td>
                          <td style="vertical-align: middle; text-align: center;">

                            <% if(order[i].status === "Pending"){ %>
                              <span class="badge rounded-pill alert-success">Pending</span>
                            <% } else if(order[i].status === "Cancelled") { %>
                              <span class="badge rounded-pill alert-danger">Candelled</span>
                            <% } else if(order[i].status === "Shipped"){%>
                            <span class="badge rounded-pill alert-success">Shipped</span>
                            <% } else if(order[i].status === "Delivered"){%>
                            <span class="badge rounded-pill alert-success">Delivered</span>
                            <% } else{%> 
                            <span class="badge rounded-pill alert-danger">Returned</span>
                                <% } %>
                            
                          </td>
                          <td style="vertical-align: middle; text-align: center;">
                            <a 
                              href="/admin/orderDetails?id=<%= order[i]._id %>"
                              class="btn bg-primary btn-sm btn-brand rounded font-sm mt-15">
                              Details
                            </a>
                          </td>
                        </tr>
                      <% } %>
                    </tbody>
                  </table>
                  
              <!-- table-responsive.// -->
            </div>
          </div>
          <!-- card-body end// -->
