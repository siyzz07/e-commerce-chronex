      <section class="content-main">
        <div class="content-header">
          <h2 class="content-title">Users list</h2>
         
        </div>
        <div class="card mb-4">
          <header class="card-header">
            <div class="row gx-3">
              <div class="col-lg-4 col-md-6 me-auto">
                <input
                  type="text"
                  placeholder="Search..."
                  class="form-control"
                />
              </div>
              
             
            </div>
          </header>
          <!-- card-header end// -->
          <div class="card-body">
            <div class="table-responsive">
              <table class="table table-hover">
                <P style="color: rgb(47, 215, 38); text-align: center;"><%= msg %></P>

                <thead>
                  <tr><Th>SI NO</Th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Status</th>
                    <th>phone</th>
                    <th>Created Date</th>
                    <th class="text-end">Action</th>
                  </tr>
                </thead>
                <tbody id="tableDatas">
                    <% for(let i=0;i<user.length;i++) {%>
                  <tr>
                    <td><%= i+1 %></td>
                    <td><%= user[i].name %></td>
                    <td><%=user[i].email %></td>
                    <td>
                      <% if(user[i].isBlocked){ %>
                        <span class="badge rounded-pill alert-danger">Inactive</span>
                     
                      <%} else{  %>
                        <span class="badge rounded-pill alert-success">Active</span>
                       
                        <% } %>
                    </td>
                    <td><%= user[i].phone %></td>
                    <td><%=user[i].created %></td>
                    <td class="text-end">

                      <% if(user[i].isBlocked){ %>
                        <a href="/admin/unblockuser?id=<%= user[i]._id %>"class="btn btn-sm bg-success btn-brand rounded font-sm mt-15">Unblock</a>

                        <% }else{ %>
                          <a href="/admin/blockuser?id=<%= user[i]._id %>"class="btn btn-sm bg-warning btn-brand rounded font-sm mt-15">Block</a>

                      <% } %>

                      <!-- <a
                        href="/admin/deleteuser?id=<%# user[i]._id %>"
                        class="btn bg-danger btn-sm btn-brand rounded font-sm mt-15"
                        >Delete</a
                      > -->
                    </td>
                  </tr>
                  <% } %>
                </tbody>
              </table>
              <!-- table-responsive.// -->
            </div>
          </div>
          <!-- card-body end// -->
        </div>
