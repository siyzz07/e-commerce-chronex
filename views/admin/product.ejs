      <section class="content-main">
        <div class="content-header">
          <h2 class="content-title">Product list</h2>
         
        </div>
        <P style="color: rgb(47, 215, 38); text-align: center;"><%= msg %></P>

        <div class="card mb-4">
          <header class="card-header">
            <div class="row gx-3">
              <div class="col-lg-4 col-md-6 me-auto">
                <input
                  type="text"
                  placeholder="Search..."
                  class="form-control"
                />
              </div>
              
              <div class="d-gridcol-lg-4 col-md-6 me-auto"> <a href="/admin/addproduct">
                <button  class="btn btn-primary"><i class="icon material-icons md-add_box"></i>Add Product</button></a>
            </div>
            </div>
          </header>
          <!-- card-header end// -->
          <div class="card-body">
            <div class="table-responsive">
              <table class="table table-hover">

                <thead>
                <tr><Th style="width: 13%;" >SI NO</Th>
                    <th style="width: 13%;">Image</th>
                    <th style="width: 13%;">Product Name</th>
                    <th style="width: 13%;">Stock</th>
                    <th style="width: 13%;">price</th>
                    <th style="width: 13%;">Status</th>
                    <th  class="text-end">Action</th>
                  </tr>

                </thead>
                <tbody id="tableDatas">
                    <% for(let i=0;i<product.length;i++) {%>
                  <tr>
                    <td><%= i+1 %></td>
                    <td style=" vertical-align: middle;">
                        <img src="/public/product_images/<%= product[i].images[0] %>" alt="Product Image" style="width: 50px; height: 50px;" />
                      </td>
                    <td><%= product[i].title %></td>
                    <td><%= product[i].stock%></td>
                    <td><%= product[i].price%></td>
                    <td>
                      <% if(product[i].isBlocked){ %>
                        <span class="badge rounded-pill alert-success">Active</span>
                     
                      <%} else{  %>
                        <span class="badge rounded-pill alert-danger">Blocked</span>
                        <% } %>
                    </td>
                   
                  
                    <td class="text-end">

                      <% if(product[i].isBlocked){ %>
                        <a href="/admin/blockproduct?id=<%= product[i]._id %>"class="btn btn-sm bg-warning btn-brand rounded font-sm mt-15">Block</a>
                        <% }else{ %>
                          <a href="/admin/unblockproduct?id=<%= product[i]._id %>"class="btn btn-sm bg-success btn-brand rounded font-sm mt-15">Unblock</a>

                      <% } %>
                      <a
                      href="/admin/editproduct?id=<%= product[i]._id %>"
                      class="btn  bg-primary btn-sm btn-brand rounded font-sm mt-15"
                      >edit</a>
                      <!-- <a
                        href="/admin/deleteproduct?id=<%#= product[i]._id %>"
                        class="btn bg-danger btn-sm btn-brand rounded font-sm mt-15"
                        >Delete</a
                      > -->
                    </td>
                  </tr>
                  <% } %>
                </tbody>
              </table>
              <!-- table-responsive.// -->
            </div>
          </div>
          <!-- card-body end// -->
        </div>
        
      </section>
